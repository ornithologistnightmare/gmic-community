(op==3)?(((x<=ox)&&(y<=oy))?0:i):
(op==4)?(((x==ox)&&(y==oy))?i:0):
(op==5)?(((x==ox)||(y==oy))?i:0):
(op==6)?((((x==ox)||(y==oy))&&((x<=ox)&&(y<=oy)))?i:0):
