fx_neon :
repeat $! l[$>]
if $2
b $2
fi
+fc 0,0,0 rv blend alpha
to_rgb rgb2hsl s c
*.. {$9^2.5}
a[0-2] c hsl2rgb
if $8
[0] l[1]
if {$1==1}
b $3
+hessian xx sqr.
+hessian.. yy sqr. +[-2,-1]
hessian.. zz sqr.. +[-2,-1] b $2
s={s} s. c +[-$s--1] sqrt.
elif {$1==2}
b $3 laplacian abs to_gray to_rgb
elif {$1==3}
b $3 gradient xyz abs blend add to_gray to_rgb
else
b $3 gradient_norm
fi
c 0,255
^ $4
c $5%,$6%
if $7 negate fi
n 0,255
if {$10!=0||$11!=100}
fx_smooth_antialias ${10-12}
fi
endl
if $9
b.. $9
fi
blend multiply,$8
fi
+b[0] {$13%*100},1 *[1] {$14*sqrt($13%)*10} -[1] {(($14*sqrt($13%)*128)-1)^(sqrt($15))}
+b[0] {$17%*100},1 *[2] {$18*sqrt($17%)*10} -[2] {(($18*sqrt($17%)*128)-1)^(sqrt($19))}
+b[0] $21,1 *[3] {($14+$18)*$21*$22}
rgb2hsl[1-3] s[1-3] c
*[2] {$16^2.5}
*[5] {$20^2.5}
*[8] 0
a[1-3] c a[2-4] c a[3-5] c hsl2rgb[1-3]
blend add,$8
if $23
rgb2ycbcr s c b[1,2] {$23%*100} a c ycbcr2rgb
fi
if $24
rgb2hsv8 100%,100%,1,1 f[1] "((j(#0,0,0,0,2)/255)^(1/$25))*255*$26" f[0] "[i0,i1,255]" hsv82rgb[0]  a c
fi
endl done
